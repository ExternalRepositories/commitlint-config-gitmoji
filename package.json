  "devDependencies": {
    "@commitlint/lint": "^11.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/sinon": "^9.0.10",
    "commitlint": "^11.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "father-build": "^1.19.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.3",
    "semantic-release-gitmoji": "^1.3.4",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
