{
  "name": "commit-gitmoji",
  "description": "",
  "private": true,
  "author": "Arvin Xu <arvinx@foxmail.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "father-build",
    "---------": "",
    "release": "yarn build && yarn release:only",
    "release:only": "lerna publish",
    "--------- ": "",
    "test": "lerna run test",
    "cov": "jest --coverage",
    "---------   ": "",
    "changelog": "conventional-changelog -p gitmoji-config -i CHANGELOG.md -s -r 0",
    "semantic-release": "semantic-release",
    "---------    ": "",
    "clean": "lerna run clean && rm -rf es lib dist build coverage .umi .eslintcache apis",
    "reinstall": "yarn clean && lerna clean &&  rm -rf node_modules && yarn",
    "postinstall": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arvinxx/commit-gitmoji.git"
  },
  "keywords": [
    "commit",
    "gitmoji",
    "emoji",
    "git"
