{
  "name": "conventional-changelog-gitmoji-config",
  "version": "1.1.2",
  "description": "a gitmoji style preset extends the angular preset",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "start": "father-build -w",
    "build": "father-build",
    "changelog": "conventional-changelog -p gitmoji-config -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arvinxx/commit-gitmoji.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "conventional-changelog",
    "gitmoji",
    "preset",
    "changelog",
    "emoji"
  ],
  "author": "Arvin Xu <arvinx@foxmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/arvinxx/commit-gitmoji/issues"
  },
  "homepage": "https://github.com/arvinxx/commit-gitmoji/tree/master/packages/changelog#readme",
  "dependencies": {
    "@gitmoji/commit-types": "^1.1.0",
    "@gitmoji/parser-opts": "^1.2.1",
    "q": "^1.5.1"
  }
}
